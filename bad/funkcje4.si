int sqr(int a)
{
	return a*a;
};

int[] map(int[] tab, (int; int) funkcja, int n)
{
	int wynik[n];
	
	for (int i = 0; i < n; i++)
		wynik[i] = funkcja(tab[i]);
	
	return n;	// zly typ zwracanej wartosci
};

int[] kwadraty = map(liczby, sqr, 10);

